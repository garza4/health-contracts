CREATE TABLE IF NOT EXISTS dbo.h_visitation_log
(
    visit_ref serial PRIMARY KEY,
    comments character varying(255) COLLATE pg_catalog."default",
    requested_funds character varying(255) COLLATE pg_catalog."default",
    service character varying(255) COLLATE pg_catalog."default",
    uid character varying(255) COLLATE pg_catalog."default"
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS dbo.h_visitation_log
    OWNER to postgres;