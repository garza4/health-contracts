CREATE TABLE IF NOT EXISTS dbo.h_user
(
    upkid bigint NOT NULL GENERATED ALWAYS AS IDENTITY ( INCREMENT 1 START 100000 MINVALUE 1 MAXVALUE 9223372036854775807 CACHE 1 ),
    uid text COLLATE pg_catalog."default" NOT NULL,
    role text COLLATE pg_catalog."default" NOT NULL,
    company text COLLATE pg_catalog."default" NOT NULL,
    first_name text COLLATE pg_catalog."default" NOT NULL,
    last_name text COLLATE pg_catalog."default" NOT NULL,
    CONSTRAINT h_user_pkey PRIMARY KEY (upkid),
    CONSTRAINT uid_unique UNIQUE (uid)
        INCLUDE(uid)
)

TABLESPACE pg_default;

ALTER TABLE IF EXISTS dbo.h_user
    OWNER to postgres;

COMMENT ON TABLE dbo.h_user
    IS 'User table for health contracts';